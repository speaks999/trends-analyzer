{
  "name": "trends-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e:install": "npx agent-browser install",
    "test:e2e": "npm run test:e2e:install && NEXT_TELEMETRY_DISABLED=1 NEXT_PUBLIC_E2E_TEST_MODE=true E2E_TEST_MODE=true E2E_PORT=3005 start-server-and-test \"next dev -p 3005\" http://127.0.0.1:3005 \"jest -c jest.e2e.config.js\"",
    "ralph": "./ralph/ralph.sh",
    "ralph:hitl": "./ralph/ralph.sh 1 hitl",
    "ralph:afk": "./ralph/ralph.sh 10 afk"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "next": "^14.2.35",
    "openai": "^4.47.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.12.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "agent-browser": "^0.4.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.35",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
